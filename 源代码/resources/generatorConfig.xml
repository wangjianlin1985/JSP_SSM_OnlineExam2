<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- 配置驱动 -->
	<classPathEntry location="E:\mysql\mysql-connector-java-5.1.35.jar" />
	<context id="context1">
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/exam" userId="root"
			password="123456" />
		<!-- 生成实体类 实体bean文件 -->
		<javaModelGenerator targetPackage="com.xhu.entity"
			targetProject="OnlineExamSystem" />
		<!-- mapper xml文件 -->
		<sqlMapGenerator targetPackage="com.xhu.mapper"
			targetProject="OnlineExamSystem" />
		<!-- mapper 接口文件 -->
		<javaClientGenerator targetPackage="com.xhu.dao"
			targetProject="OnlineExamSystem" type="XMLMAPPER" />

		<!-- 具体映射表 -->
		<!-- <table tableName="sys_user" domainObjectName="SysUser" enableCountByExample="false" 
			enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" 
			selectByExampleQueryId="false" ></table> -->
			<table tableName="shortanswer" domainObjectName="Shortanswer"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false"></table>
	</context>
</generatorConfiguration>